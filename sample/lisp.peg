
Stmt    <- Expr;
Expr    <- {'(' _? ($Expr (_ $Expr)*)? ')'}:S  / Integer / Symbol;
Integer <- { [0-9]+ } :num;
Symbol  <- { [a-zA-Z]+ } :name;
Symbol  <- { '+'  /  '-' /  '*' / '/' / '%' / '<' / '>' } :symbol;
_       <- [ \t\n];

Expr    <- {'(+' _+ $Expr _+ $Expr _? ')' } :add;
Expr    <- {'(+' _+ $Expr _? ')' } :plus;

